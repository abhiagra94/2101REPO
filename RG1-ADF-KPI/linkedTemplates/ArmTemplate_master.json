{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "RG1-ADF-KPI"
		},
		"LS_AzureBlobStorageA_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureBlobStorageA'"
		},
		"LS_DataLakeStorageA_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DataLakeStorageA'"
		},
		"LS_SQL_Ecentricity_A_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_Ecentricity_A'",
			"defaultValue": "Integrated Security=True;Data Source=ECCENTRICITY\\SQLEXPRESS;Initial Catalog=sqlpool"
		},
		"LS_SQL_Ecentricity_A_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQL_Ecentricity_A'"
		},
		"LS_DataLakeStorageA_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://lg2.dfs.core.windows.net/"
		},
		"LS_SQL_Ecentricity_A_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "anumeyachandra@live.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "RG1-ADF-KPI_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureBlobStorageA_connectionString": {
						"value": "[parameters('LS_AzureBlobStorageA_connectionString')]"
					},
					"LS_DataLakeStorageA_accountKey": {
						"value": "[parameters('LS_DataLakeStorageA_accountKey')]"
					},
					"LS_SQL_Ecentricity_A_connectionString": {
						"value": "[parameters('LS_SQL_Ecentricity_A_connectionString')]"
					},
					"LS_SQL_Ecentricity_A_password": {
						"value": "[parameters('LS_SQL_Ecentricity_A_password')]"
					},
					"LS_DataLakeStorageA_properties_typeProperties_url": {
						"value": "[parameters('LS_DataLakeStorageA_properties_typeProperties_url')]"
					},
					"LS_SQL_Ecentricity_A_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_Ecentricity_A_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}